#include <iostream>

using namespace std;


int main(){
    int sk, suma;
    cout<<" sk = ";
    cin>>sk;


    for(int i=1; i<sk; i++) {
    //cout<<i;
            suma=0;
        for(int j=1; j<i; j++){

                suma+=j;
                cout<<j<<"+";



    }
    if(i>1){cout<<"suma= "<<suma;}
cout<<endl;
   }

return 0;
}
